ine.js","original":{"line":6,"column":22}},{"generated":{"line":67,"column":25},"source":"src/components/spot-it/spot-it-engine.js","original":{"line":6,"column":4}},{"generated":{"line":68,"column":0},"source":"src/components/spot-it/spot-it-engine.js","original":{"line":7,"column":4}},{"name":"resetTwoCards","generated":{"line":68,"column":7},"source":"src/components/spot-it/spot-it-engine.js","original":{"line":7,"column":9}},{"generated":{"line":68,"column":20},"source":"src/components/spot-it/spot-it-engine.js","original":{"line":7,"column":4}},{"generated":{"line":69,"column":0},"source":"src/components/spot-it/spot-it-engine.js","original":{"line":8,"column":3}},{"name":"SpotItEngine","generated":{"line":71,"column":18},"source":"src/components/spot-it/spot-it-engine.js","original":{"line":4,"column":21}}],"sources":{"src/components/spot-it/spot-it-engine.js":"import card1Image from './01.png';\nimport card2Image from './55.png';\n\nexport default class SpotItEngine {\n  constructor() {\n    this.isGameOver = false;\n    this.resetTwoCards();\n  }\n\n  resetTwoCards = () => {\n      this.card1 = [\"igloo\", \"dragon\", \"art\", \"lightning\", \"knight\", \"man\", \"dog\", \"padlock\"],\n      this.card2 = [\"padlock\", \"lips\", \"anchor\", \"music-note\", \"flower\", \"exclamation-point\", \"car\", \"key\"]\n      this.card1.sort(() => Math.random() < .5 ? -1 : 1);\n      this.card2.sort(() => Math.random() < .5 ? -1 : 1);\n      this.card1Image = card1Image;\n      this.card2Image = card2Image;\n      this.guessesLeft = 3;\n    }\n  \n  getState = () => {\n    return {\n      card1: this.card1,\n      card2: this.card2,\n      guessesLeft: this.guessesLeft,\n      isGameOver: this.isGameOver,\n      card1Image: this.card1Image,\n      card2Image: this.card2Image,\n    }\n  }\n\n  isMatch = (symbol) => {\n    if (this.isGameOver) {\n      return;\n    }\n\n    this.guessesLeft--;\n    if (this.guessesLeft === 0) {\n      this.isGameOver = true;\n    }\n\n    let isInCard1 = this.card1.includes(symbol);\n    let isInCard2 = this.card2.includes(symbol);\n    const isMatch = isInCard1 && isInCard2;\n    if (isMatch) {\n      this.resetTwoCards();\n    }\n    return isMatch;\n  }\n}"},"lineCount":null}},"hash":"87e4a639bf38d8a4a745941f2e979e97","cacheData":{"env":{}}}